package com.fiveone.edm.database.entity;

import java.sql.Timestamp;
import java.util.List;

/**
 * 邮件发送策略实体类
 * @company: 51jrq
 * @author: lhw
 * @time: 2017年1月12日 下午1:56:14
 * @version: 1.0
 * @since: JDK1.7
 */
public class EmailSendPolicy {
	
	// 主键
	private Integer id;

	// 邮件类型
	private String emailType;

	// 一天最大发送量
	private Integer dayMax;

	// 当天已发送量
	private Integer dayNum;

	// 每小时最大发送量
	private Integer hourMax;

	// 当前小时发送量
	private Integer hourNum;

	// 每分钟发送最大值
	private Integer minuteMax;

	// 当前分钟已发送值
	private Integer minuteNum;

	// 当天开始时间
	private Timestamp startDayTime;

	// 当天结束时间
	private Timestamp endDayTime;

	// 当前小时开始时间
	private Timestamp startHourTime;

	// 当前小时结束时间
	private Timestamp endHourTime;

	// 当前分钟开始时间
	private Timestamp startMinuteTime;

	// 当前分钟结束时间
	private Timestamp endMinuteTime;

	// 数据更新时间
	private Timestamp updateTime;

	// 数据创建时间
	private Timestamp createTime;

	// 客户端IP地址
	private String ip;

	// 客户端端口号
	private String port;

	// 客户端编号
	private String clientId;

	// 是否停止发送
	private boolean isStop;

	// 停止时间（毫秒为单位）
	private long stopTime;

	// 开始停止时间
	private String stopStartTime;

	// 发送客户端列表
	private List<String[]> clients;

	public Integer getId() {
		return id;
	}

	public void setId(Integer id) {
		this.id = id;
	}

	public String getEmailType() {
		return emailType;
	}

	public void setEmailType(String emailType) {
		this.emailType = emailType;
	}

	public Integer getDayMax() {
		return dayMax;
	}

	public void setDayMax(Integer dayMax) {
		this.dayMax = dayMax;
	}

	public Integer getDayNum() {
		return dayNum;
	}

	public void setDayNum(Integer dayNum) {
		this.dayNum = dayNum;
	}

	public Integer getHourMax() {
		return hourMax;
	}

	public void setHourMax(Integer hourMax) {
		this.hourMax = hourMax;
	}

	public Integer getHourNum() {
		return hourNum;
	}

	public void setHourNum(Integer hourNum) {
		this.hourNum = hourNum;
	}

	public Integer getMinuteMax() {
		return minuteMax;
	}

	public void setMinuteMax(Integer minuteMax) {
		this.minuteMax = minuteMax;
	}

	public Integer getMinuteNum() {
		return minuteNum;
	}

	public void setMinuteNum(Integer minuteNum) {
		this.minuteNum = minuteNum;
	}

	public Timestamp getStartDayTime() {
		return startDayTime;
	}

	public void setStartDayTime(Timestamp startDayTime) {
		this.startDayTime = startDayTime;
	}

	public Timestamp getEndDayTime() {
		return endDayTime;
	}

	public void setEndDayTime(Timestamp endDayTime) {
		this.endDayTime = endDayTime;
	}

	public Timestamp getStartHourTime() {
		return startHourTime;
	}

	public void setStartHourTime(Timestamp startHourTime) {
		this.startHourTime = startHourTime;
	}

	public Timestamp getEndHourTime() {
		return endHourTime;
	}

	public void setEndHourTime(Timestamp endHourTime) {
		this.endHourTime = endHourTime;
	}

	public Timestamp getStartMinuteTime() {
		return startMinuteTime;
	}

	public void setStartMinuteTime(Timestamp startMinuteTime) {
		this.startMinuteTime = startMinuteTime;
	}

	public Timestamp getEndMinuteTime() {
		return endMinuteTime;
	}

	public void setEndMinuteTime(Timestamp endMinuteTime) {
		this.endMinuteTime = endMinuteTime;
	}

	public Timestamp getUpdateTime() {
		return updateTime;
	}

	public void setUpdateTime(Timestamp updateTime) {
		this.updateTime = updateTime;
	}

	public Timestamp getCreateTime() {
		return createTime;
	}

	public void setCreateTime(Timestamp createTime) {
		this.createTime = createTime;
	}

	public String getIp() {
		return ip;
	}

	public void setIp(String ip) {
		this.ip = ip;
	}

	public String getPort() {
		return port;
	}

	public void setPort(String port) {
		this.port = port;
	}

	public String getClientId() {
		return clientId;
	}

	public void setClientId(String clientId) {
		this.clientId = clientId;
	}

	public boolean isStop() {
		return isStop;
	}

	public void setStop(boolean isStop) {
		this.isStop = isStop;
	}

	public long getStopTime() {
		return stopTime;
	}

	public void setStopTime(long stopTime) {
		this.stopTime = stopTime;
	}

	public String getStopStartTime() {
		return stopStartTime;
	}

	public void setStopStartTime(String stopStartTime) {
		this.stopStartTime = stopStartTime;
	}

	public List<String[]> getClients() {
		return clients;
	}

	public void setClients(List<String[]> clients) {
		this.clients = clients;
	}

}
 